{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#services/*": "./app/services/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#database/*": "./database/*.js"
  },
  "scripts": {
    "dev": "tsx --watch bin/server.ts",
    "build": "node ace build --production",
    "start": "node build/bin/server.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node ace test",
    "migrate": "tsx database/migrator.ts",
    "migrate:down": "tsx database/migrator.ts down",
    "migrate:fresh": "tsx database/migrator.ts fresh",
    "seed": "tsx database/seeds/run.ts",
    "setup:db": "tsx database/migrator.ts && tsx database/seeds/run.ts",
    "setup:fresh": "tsx database/migrator.ts fresh && tsx database/seeds/run.ts"
  },
  "dependencies": {
    "@adonisjs/core": "^6.17.0",
    "@adonisjs/cors": "^2.2.1",
    "@vinejs/vine": "^4.3.0",
    "dotenv": "^17.2.4",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "shared": "workspace:*",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.0",
    "@types/jsonwebtoken": "^9.0.7",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2"
  }
}
